PORT=3001
MONGODB_URI=mongodb://localhost:27017/websoroban-ide
NODE_ENV=development

# Redis configuration
# Use 'localhost' for local development, 'redis' for Docker Compose
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Worker concurrency settings
COMPILE_WORKER_CONCURRENCY=2
DEPLOY_WORKER_CONCURRENCY=2

# Worker type (for worker containers)
WORKER_TYPE=compile

# Email configuration (for sending invite codes)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_FROM=your-email@gmail.com
SMTP_FROM_NAME=WebSoroban IDE
FRONTEND_URL=https://websoroban.in